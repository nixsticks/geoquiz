.row.quiz-container
  #map.col-md-7.nogutter

  #pane.col-md-5
    .card.center
      .front
        .input-container
          .title
            %h1 Where in the world are we?
          = render 'answers/form'
          .button#next
            %p Next/Skip

        .info-container
          #notification
          %a{href: "#", class: "flip"} See what other people thought.
      .back
        #countrydata
        %a{href: "#", class: "flip"} back
      
= javascript_include_tag :quiz